
# include <stdio.h>

int main(){
    char name[50];
    int age;
    float mark;


    printf("Enter your name :");
    scanf("%s", name);

    printf("Enter your age : ");
    scanf("%d",&age);

    printf("Enter your mark :");
    scanf("%f",&mark);


    printf("name is : %s \n",name);
    printf("age is :%d \n", age);
    printf("mark is :%f\n " , mark);



    return 0;
}
